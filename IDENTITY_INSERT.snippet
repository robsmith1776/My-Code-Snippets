<CodeSnippet Format="1.0.0">
<Header>
<Title>IDENTITY_INSERT Template</Title>
       <Shortcut></Shortcut>
<Description>Example Snippet for IDENTITY_INSERT</Description>
<Author>RobertSmith</Author>
<SnippetTypes>
      <SnippetType>Expansion</SnippetType>
</SnippetTypes>
</Header>
<Snippet>
<Declarations>
  <Literal>
    <ID>SchemaName</ID>
    <ToolTip>Name of the schema</ToolTip>
    <Default>Bullhorn1</Default>
  </Literal>
  <Literal>
  	<ID>TableName</ID>
  	<ToolTip>Name of the table</ToolTip>
  	<Default>TableName</Default>
	</Literal>
	  <Literal>
  	<ID>Database</ID>
  	<ToolTip>Name of the Database</ToolTip>
  	<Default>Database</Default>
	</Literal>
</Declarations>
<!-- A comment here must be an XML comment, not a T-SQL comment -->
<Code Language="SQL"><![CDATA[SET IDENTITY_INSERT [$Database$].[$SchemaName$].[$TableName$] ON]]>
</Code>
</Snippet>
</CodeSnippet>
</CodeSnippets>
